<%= form_with url: wines_path do |f|%>
  <div>
    <%= f.label :wine_name %>
    <%= f.text_field :wine_name %>
  </div>
  <div>
    <%= f.label :wine_name_kana %>
    <%= f.text_field :wine_name_kana %>
  </div>
  <div>
    <%= f.label :winary_name %>
    <%= f.text_field :winary_name %>
  </div>
  <div>
    <%= f.label :winary_name_kana %>
    <%= f.text_field :winary_name_kana %>
  </div>
  <div>
    <%= f.label :grape_name %>
    <%= f.text_field :grape_name %>
  </div>
  <div>
    <%= f.label :vintage %>
    <%= f.text_field :vintage %>
  </div>
  <div>
    <%= f.label :comment %>
    <%= f.text_field :comment %>
  </div>
  <div>
    <%= f.label :purchase_price %>
    <%= f.text_field :purchase_price %>
  </div>
  <div>
    <%= f.label :selling_price %>
    <%= f.text_field :selling_price %>
  </div>
  <div>
    <%= f.label :stock %>
    <%= f.text_field :stock %>
  </div>
  <div>
    <%= f.label :country %>
    <%= f.collection_select(:country_id, Country.all, :id, :name) %>
  </div>
  <div>
    <%= f.label :state%>
    <%= f.text_field :state %>
  </div>
  <div>
    <%= f.label :company_name%>
    <%= f.text_field :company_name %>
  </div>
  <%= f.submit%>
<% end %>